pipeline {
    agent any
    tools {
            maven 'Maven3'
    }
    stages {
        stage('Build') {
            steps {
                   echo 'building process is started'
                   sh 'mvn -f user-service/pom.xml clean compile'
            }
        }
        stage('Test') {
            steps {
                echo 'test process is started'
                sh 'mvn -f user-service/pom.xml clean test'
            }
        }
//         stage('Deploy') {
